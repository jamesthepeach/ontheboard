<div id="left">
	<div class="navigation">
		<%= link_to "Create a new board.",new_board_url%>
	</div><!--end navigation-->
</div><!--end left-->

<div id="right">
	
</div><!--end left-->

<div id="center">
	<h1><%= @page.user.login %></h1>
	<h3>Your Boards</h3>
	<div id="boards" class="page_object">
		<ul class="board_list">
			<% @page.user.boards.each do |board|%>
				<li><%= link_to "#{board.title}", board %></li>
			<% end %>
		</ul>
	</div><!--end boards-->
	<h3>Your Tallies</h3>
	<div id="tallies" class="page_object">
		<ul class="tally_list">
				<li><a>Flag Football League</a> </li>
				<li><a>year end sales campaign </a></li>
				<li><a>happy hour at the fox and the hound</a></li>
		</ul>
	</div><!--end boards-->

</div><!--end left-->