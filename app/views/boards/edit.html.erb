<div id="left">
	
</div><!--end left-->
<div id="right">
	
</div><!--end right-->
<div id="center">
	<ul class="form">
	<% form_for(@board, :html => {:class => 'user_form'}) do |f| %>
	  <%= f.error_messages %>
		<li>
	    <%= f.label :title %>
	    <%= f.text_field :title %>
		</li>
		<li>
	    <%= f.label :description %>
	    <%= f.text_field :description %>
		</li>

		<li>
	    <%= f.label :is_public %>
	    <%= f.check_box :is_public %>
		</li>
		<li>
	    <%= f.submit "Update" %>
		<li>
		<li class="cancel">
		<%= link_to "Cancel", @board%>
		</li>
	<% end %>
	</ul>
</div><!--end center-->
